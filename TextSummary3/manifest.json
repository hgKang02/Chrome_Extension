{
    "manifest_version": 3,
    "name": "Text Summarizer",
    "description": "Summarize the web page for you",
    "version": "1.0",
    "action": {
      "default_popup": "popup.html",
      "default_icon": "icon.png"
    },
    "permissions": [
      "storage",
      "tabs",
      "scripting",
      "activeTab"
    ],
    "content_security_policy": {
      "extension_pages": "script-src 'self'; object-src 'self'"
    }
  }
  